page{
  background: #fff;
}
.box{
  width: 100%;
  height: 40vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.box_img{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  margin-bottom: 30rpx;
}
.box_img_pic{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.box_title{
  height: 70rpx;
  line-height: 70rpx;
  color: #010101;
  font-size: 38rpx;
  font-weight: bold;
}
.box_version{
  font-size: 34rpx;
  color: #1b1b1b;
  height: 70rpx;
  line-height: 70rpx;
}
.signature{
  width: 100%;
  height: 60vh;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
}
.signature_logo{
  width: 250rpx;
  height: 120rpx;
}
.signature_logo_pic{
  width: 250rpx;
  height: 120rpx;
}
.signature_title{
  width: 400rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin-bottom: 150rpx;
  color: #c6c6c6;
  font-size: 28rpx;
  text-align: center;
}
